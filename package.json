{
	"name": "encore-prisma-example",
	"private": true,
	"version": "0.0.1",
	"description": "Encore + Prisma Typescript Example",
	"license": "MPL-2.0",
	"type": "module",
	"scripts": {
		"dev": "encore run",
		"postinstall": "npx prisma generate --schema=database/prisma/schema.prisma",
		"fix": "biome check --write",
		"check": "biome check",
		"check:errors": "biome check --diagnostic-level=error",
		"typecheck": "tsc --noEmit",
		"clean": "rimraf node_modules pnpm-lock.yaml dist",
		"db:push": "prisma db push --schema=database/prisma/schema.prisma",
		"db:generate": "prisma generate --schema=database/prisma/schema.prisma",
		"db:migrate": "prisma migrate dev --schema=database/prisma/schema.prisma",
		"db:reset": "prisma migrate reset --schema=database/prisma/schema.prisma"
	},
	"devDependencies": {
		"@biomejs/biome": "2.0.6",
		"@types/node": "^20.5.7",
		"prisma": "^6.11.1",
		"rimraf": "^6.0.1",
		"typescript": "^5.2.2"
	},
	"dependencies": {
		"@prisma/client": "^6.11.1",
		"encore.dev": "^1.48.8"
	},
	"optionalDependencies": {
		"@rollup/rollup-linux-x64-gnu": "^4.13.0"
	}
}
